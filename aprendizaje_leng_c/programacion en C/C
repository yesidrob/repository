#include <stdio.h>
int main() {
    float examen1, examen2;
    printf("ingrese la nota del primer examen: ");
    scanf("%f",&examen1);
    printf("ingrese la nota del segundo examen: ");
    scanf("%f",&examen2);

    float notaFinal;
    notaFinal = (examen1 + examen2) / 2;
    printf("HOLA MUNDO!");
    printf("La nota final es: %f",notaFinal);
    return 0;
}


